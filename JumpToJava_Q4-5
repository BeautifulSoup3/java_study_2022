public class Sample {
    public static void main(String[] args) {
        int[] marks = {70, 60, 55, 75, 95, 90, 80, 80, 85, 100};
        int sum = 0;
        int average;
        for (int number: marks){
            sum += number;
        }
        average = sum / 10;
        System.out.println(average);
    }
}
